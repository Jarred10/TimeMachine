{
    "version": "0.1.0",
    "command": "javac",
    "showOutput": "silent",
    "isShellCommand": true,
    "args": ["-d","${Time Machine}\\bin","${workspaceRoot}\\src\\*.java"],
    "problemMatcher": {
        "owner": "external",
        "fileLocation": ["absolute"],
        "pattern": [
        {
            "regexp": "^(.+\\.java):(\\d):(?:\\s+(error)):(?:\\s+(.*))$",
            "file": 1,
            "location": 2,
            "severity": 3,
            "message": 4
        }]
    }
}